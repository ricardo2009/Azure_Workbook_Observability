                        "type": 3,
                        "content": {
                          "version": "KqlItem/1.0",
                          "query": "resources\r\n| where type == \"microsoft.storage/storageaccounts\"\r\n| summarize count() by ['Primary Location']=tostring(properties.primaryLocation)",
